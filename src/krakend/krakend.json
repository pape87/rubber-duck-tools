{
  "$schema": "https://www.krakend.io/schema/v3.json",
  "version": 3,
  "extra_config": {},
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "name": "rubber-duck-tools",
  "port": 8080,
  "endpoints": [
    {
      "endpoint": "/v1/status",
      "method": "GET",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/auth",
          "encoding": "json",
          "method": "GET",
          "host": [
            "http://auth-service:3000"
          ]
        },
        {
          "url_pattern": "/user",
          "encoding": "json",
          "method": "GET",
          "host": [
            "http://user-service:3000"
          ]
        }
      ]
    },
    {
      "endpoint": "/v1/login",
      "input_query_strings": ["code"],
      "method": "GET",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/auth/getToken",
          "encoding": "json",
          "method": "GET",
          "host": [
            "http://auth-service:3000"
          ]
        }
      ]
    }
  ]
}